{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "./src/**/*_client.ts"
    ],
    "watch": [
      "./src",
      "./src/app/index.ts"
    ],
    "ext": ".ts"
  },
  "scripts": {
    "up": "docker-compose -f ./dev/docker-compose.yml up -d --no-recreate --remove-orphans && npm run logs:dev",
    "down": "docker-compose -f ./dev/docker-compose.yml down --rmi local",
    "bootstrap": "make bootstrap",
    "clean": "make clean",
    "build": "make build",
    "watch": "make watch",
    "logs:dev": "docker-compose -f ./dev/docker-compose.yml logs -f",
    "logs:staging": "heroku logs --tail -a mospolytech-words-staging",
    "logs:production": "heroku logs --tail -a mospolytech-words"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@webpack-cli/generators": "^2.0.0",
    "css-loader": "^5.2.4",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.12",
    "postcss-loader": "^5.2.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
